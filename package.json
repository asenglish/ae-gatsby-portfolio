{
  "name": "ae-photo",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "rm --rf ./.cache && gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "upload": "node ./.utils/uploader.js ~/Pictures/LR\\ Cloudinary"
  },
  "dependencies": {
    "gatsby": "^2.32.13",
    "gatsby-image": "^2.2.18",
    "gatsby-plugin-remote-images": "^1.0.7",
    "gatsby-plugin-sharp": "^2.2.21",
    "gatsby-source-filesystem": "^2.1.22",
    "gatsby-transformer-json": "^2.2.8",
    "gatsby-transformer-sharp": "^2.2.14",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "devDependencies": {
    "cli-progress": "^3.1.0",
    "cloudinary": "^1.15.0",
    "prettier": "^1.18.2",
    "uniqid": "^5.0.3"
  }
}
